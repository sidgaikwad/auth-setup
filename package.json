{
  "name": "@sidgaikwad/auth-setup",
  "version": "1.0.0",
  "description": "Framework-agnostic CLI for setting up authentication with Better Auth, Clerk, and more",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "auth-setup": "./bin/cli.js"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "bun run src/index.ts",
