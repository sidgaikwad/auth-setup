  "repository": {
    "type": "git",
    "url": "https://github.com/sidgaikwad/auth-setup.git"
  },
  "bugs": {
    "url": "https://github.com/sidgaikwad/auth-setup/issues"
  },
  "homepage": "https://github.com/sidgaikwad/auth-setup#readme",
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "chalk": "^5.3.0",
    "execa": "^8.0.1"
